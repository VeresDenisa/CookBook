<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ckbk.sre.controllers.MyManagerStatisticsController">
    <fx:include source="Sidebar.fxml" />

   <LineChart fx:id="chart" layoutX="181.0" layoutY="30.0" prefHeight="440.0" prefWidth="647.0">
     <xAxis>
       <CategoryAxis side="BOTTOM" />
     </xAxis>
     <yAxis>
       <NumberAxis side="LEFT" />
     </yAxis>
   </LineChart>

    <Button layoutX="73.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#handleGoOrders" prefHeight="41.0" prefWidth="53.0">
        <graphic>
            <ImageView fitHeight="34.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@images/icon/icon_orders.png" />
            </ImageView>
        </graphic>
    </Button>

</AnchorPane>
